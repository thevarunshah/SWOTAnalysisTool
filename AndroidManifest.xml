<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.swotanalysistool"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="22" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/swot_logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.thevarunshah.swotanalysistool.HomeScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.thevarunshah.swotanalysistool.NewSWOTScreen"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.thevarunshah.swotanalysistool.SavedSWOTsScreen"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>
